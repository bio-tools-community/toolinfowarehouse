				<xs:element ref="resource" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Description of a software entity, e.g. a tool, data service, package etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Interface">
