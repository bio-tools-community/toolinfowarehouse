							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="Text"/>
					</xs:simpleType>
				</xs:element>
				<xs:element name="topic" type="EDAMTopic" maxOccurs="unbounded">
