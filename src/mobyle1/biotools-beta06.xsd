					<xs:annotation>
						<xs:documentation>Canonical resource name</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="ResourceName"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="homepage" type="URL">
					<xs:annotation>
						<xs:documentation>Resource homepage (URL)</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="http://resourcename.org"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="version" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Resource version e.g. version number</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="Version 1"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:whiteSpace value="collapse"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="collection" type="xs:token" minOccurs="0" maxOccurs="unbounded">
