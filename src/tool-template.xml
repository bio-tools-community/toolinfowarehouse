<?xml version="1.0" encoding="UTF-8"?>
<tools xsi:schemaLocation="http://biotoolsregistry.org biotools-beta04.xsd" xmlns="http://biotoolsregistry.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tool>
		<name>{{collectionName}}_{{name}}</name>
		<homepage>{{homepage}}</homepage>
		<version>{{version}}</version>
		<collectionName>{{collectionName}}</collectionName>
                {{#uses}}
		<uses>{{.}}</uses>
                {{/uses}}
		<softwareType uri="http://www.ebi.ac.uk/swo/SWO_0000000">Command line</softwareType>
		<description>{{description}}</description>
                <docs>
                  <docsHome>{{docs}}</docsHome>
                </docs>
                <publications>
                  {{#publications}}
                  <publicationOtherID>{{.}}</publicationOtherID>
                  {{/publications}}
                </publications>
                <credits>
                  {{#authors}}
                  <contributor>{{.}}</contributor>
                  {{/authors}}
                </credits>
	</tool>
</tools>
