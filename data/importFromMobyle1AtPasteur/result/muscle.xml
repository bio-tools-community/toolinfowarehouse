    <description>MUSCLE is a program for creating multiple alignments of amino acid or nucleotide sequences.</description>
    <topic uri="http://edamontology.org/topic_0182">Sequence alignment</topic>
    <function>
      <functionName uri="http://edamontology.org/operation_0492">Multiple sequence alignment construction</functionName>
      <input>
        <dataType uri="http://edamontology.org/data_2044">Sequence</dataType>
        <dataFormat uri="http://edamontology.org/format_2200">FASTA-like (text)</dataFormat>
        <dataHandle>sequence</dataHandle>
      </input>
      <input>
        <dataType uri="http://edamontology.org/data_0863">Sequence alignment</dataType>
        <dataFormat uri="http://edamontology.org/format_2200">FASTA-like (text)</dataFormat>
