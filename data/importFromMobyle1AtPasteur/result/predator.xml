<?xml version="1.0"?>
<resources xmlns="http://biotoolsregistry.org" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xsi:schemaLocation="http://biotoolsregistry.org/biotools-beta06.xsd">
  <resource>
    <name>Mobyle_predator_2.1.2</name>
    <homepage>http://mobyle.pasteur.fr#forms::predator</homepage>
    <version>2.1.2</version>
    <collection>Mobyle</collection>
    <resourceType uri="http://www.ebi.ac.uk/swo/SWO_0000000">Tool (analysis)</resourceType>
    <interface>
      <interfaceType>Web UI</interfaceType>
    </interface>
    <description>Protein secondary structure prediction from a single sequence or a set of sequences</description>
    <topic uri="http://edamontology.org/topic_0172">Protein structure prediction</topic>
    <topic uri="http://edamontology.org/topic_0694">Protein secondary structure</topic>
    <function>
      <functionName uri="http://edamontology.org/operation_0267">Protein secondary structure prediction</functionName>
      <input>
        <dataType uri="http://edamontology.org/data_2044">Sequence</dataType>
        <dataFormat uri="http://edamontology.org/format_2200">FASTA-like (text)</dataFormat>
        <dataHandle>sequences</dataHandle>
      </input>
      <input>
        <dataType uri="http://edamontology.org/data_0863">Sequence alignment</dataType>
        <dataFormat uri="http://edamontology.org/format_2200">FASTA-like (text)</dataFormat>
        <dataHandle>alignment</dataHandle>
      </input>
      <input>
        <dataType uri="http://edamontology.org/data_2048">Report</dataType>
        <dataHandle>stride_file</dataHandle>
      </input>
      <output>
        <dataType uri="http://edamontology.org/data_2048">Report</dataType>
        <dataHandle>stderr</dataHandle>
      </output>
    </function>
    <contact>
      <contactEmail>mobyle@pasteur.fr</contactEmail>
      <contactRole>General</contactRole>
    </contact>
    <publications/>
    <credits>
      <creditsContributor>D. Frishman &amp; P. Argos</creditsContributor>
    </credits>
