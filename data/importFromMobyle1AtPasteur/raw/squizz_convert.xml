<program>
  <head>
    <name>squizz_convert</name>
    <package xml:base="Entities/squizz_package.xml">
      <name>squizz</name>
      <version>0.99b</version>
      <doc>
        <title>squizz</title>
        <description>
          <text lang="en">Sequence/Alignment format checker/converter.</text>
        </description>
        <sourcelink>ftp://ftp.pasteur.fr/pub/gensoft/projects/squizz/</sourcelink>
      </doc>
    </package>
    <version>0.99b</version>
    <doc>
      <title>SQUIZZ</title>
      <description>
        <text lang="en">Sequence/Alignment format converter</text>
      </description>
      <authors>N. Joly</authors>
      <doclink>http://bioweb2.pasteur.fr/docs/squizz/seqfmt.html</doclink>
      <doclink>http://bioweb2.pasteur.fr/docs/squizz/alifmt.html</doclink>
    </doc>
    <command>squizz</command>
    <category>alignment:formatter</category>
    <category>sequence:formatter</category>
    <interface type="form" generated="true">
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="seq">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Sequence section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <fieldset class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="1,n" data-formats="EMBL FASTA GCG GENBANK IG NBRF CODATA RAW SWISSPROT " data-parametername="infile_seq">
            <legend>Input</legend>
            <xhtml:textarea name="infile_seq" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
          <label class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="convert_seq">Convert sequence into sequence format (-c)<xhtml:select name="convert_seq">
              <xhtml:option value="null" selected="selected">Choose the output format</xhtml:option>
              <xhtml:option value="EMBL">EMBL</xhtml:option>
              <xhtml:option value="FASTA">FASTA</xhtml:option>
              <xhtml:option value="GCG">GCG</xhtml:option>
              <xhtml:option value="GDE">GDE</xhtml:option>
              <xhtml:option value="GENBANK">GENBANK</xhtml:option>
              <xhtml:option value="IG">IG</xhtml:option>
              <xhtml:option value="NBRF">NBRF</xhtml:option>
              <xhtml:option value="CODATA">CODATA</xhtml:option>
              <xhtml:option value="RAW">RAW</xhtml:option>
              <xhtml:option value="SWISSPROT">SWISSPROT</xhtml:option>
            </xhtml:select>
          </label>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="aln">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Alignment section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <fieldset class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Alignment" data-datatype-superclass="" data-biotype="" data-card="" data-formats="CLUSTAL PHYLIPI PHYLIPS MSF NEXUS STOCKHOLM FASTA MEGA " data-parametername="infile_aln">
            <legend>Input</legend>
            <xhtml:textarea name="infile_aln" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
          <label class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="convert_aln">Convert alignment into alignment format (-c)<xhtml:select name="convert_aln">
              <xhtml:option value="null" selected="selected">Choose the output format</xhtml:option>
              <xhtml:option value="CLUSTAL">CLUSTAL</xhtml:option>
              <xhtml:option value="FASTA">FASTA</xhtml:option>
              <xhtml:option value="MEGA">MEGA</xhtml:option>
              <xhtml:option value="MSF">MSF</xhtml:option>
              <xhtml:option value="NEXUS">NEXUS</xhtml:option>
              <xhtml:option value="PHYLIPI">PHYLIP Interleaved</xhtml:option>
              <xhtml:option value="PHYLIPS">PHYLIP Sequential</xhtml:option>
              <xhtml:option value="STOCKHOLM">STOCKHOLM</xhtml:option>
            </xhtml:select>
          </label>
          <label class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="convert_seq2">Convert alignment into sequence format (-c)<xhtml:select name="convert_seq2">
              <xhtml:option value="null" selected="selected">Choose the output format</xhtml:option>
              <xhtml:option value="EMBL">EMBL</xhtml:option>
              <xhtml:option value="FASTA">FASTA</xhtml:option>
              <xhtml:option value="GCG">GCG</xhtml:option>
              <xhtml:option value="GDE">GDE</xhtml:option>
              <xhtml:option value="GENBANK">GENBANK</xhtml:option>
              <xhtml:option value="IG">IG</xhtml:option>
              <xhtml:option value="NBRF">PIR/NBRF</xhtml:option>
              <xhtml:option value="CODATA">PIR/CODATA</xhtml:option>
              <xhtml:option value="RAW">RAW</xhtml:option>
              <xhtml:option value="SWISSPROT">SWISSPROT</xhtml:option>
            </xhtml:select>
          </label>
        </xhtml:div>
      </fieldset>
    </interface>
    <interface type="job_input" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="seq">
        <xhtml:legend>Sequence section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="infile_seq" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="1,n" data-formats="EMBL FASTA GCG GENBANK IG NBRF CODATA RAW SWISSPROT ">
            <xhtml:i>
				(Sequence)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="convert_seq" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="aln">
        <xhtml:legend>Alignment section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="infile_aln" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Alignment" data-datatype-superclass="" data-biotype="" data-card="" data-formats="CLUSTAL PHYLIPI PHYLIPS MSF NEXUS STOCKHOLM FASTA MEGA ">
            <xhtml:i>
				(Alignment)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="convert_aln" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="convert_seq2" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
    </interface>
    <interface type="job_output" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="seq">
        <xhtml:legend>Sequence section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="seq_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="1,n" data-formats="&#10;               ">
            <xhtml:i>
				(Sequence)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="aln">
        <xhtml:legend>Alignment section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="aln_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Alignment" data-datatype-superclass="" data-biotype="" data-card="" data-formats="&#10;               ">
            <xhtml:i>
				(Alignment)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="seq2_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="" data-formats="&#10;               ">
            <xhtml:i>
				(Sequence)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stderr" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
    </interface>
  </head>
  <parameters>
    <paragraph>
      <name>seq</name>
      <prompt lang="en">Sequence section</prompt>
      <precond>
        <code proglang="perl">not $infile_aln or ($infile_seq and $infile_aln)</code>
        <code proglang="python">not infile_aln or (infile_seq and infile_aln)</code>
      </precond>
      <parameters>
        <parameter ismandatory="1" issimple="1">
          <name>infile_seq</name>
          <prompt lang="en">Input</prompt>
          <type>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>EMBL</dataFormat>
            <dataFormat>FASTA</dataFormat>
            <dataFormat>GCG</dataFormat>
            <dataFormat>GENBANK</dataFormat>
            <dataFormat>IG</dataFormat>
            <dataFormat>NBRF</dataFormat>
            <dataFormat>CODATA</dataFormat>
            <dataFormat>RAW</dataFormat>
            <dataFormat>SWISSPROT</dataFormat>
            <card>1,n</card>
          </type>
          <format>
            <code proglang="perl">" ($value)"</code>
            <code proglang="python">" "+str(value)</code>
          </format>
          <ctrl>
            <message>
              <text lang="en">You must enter either a sequence or a alignment.</text>
            </message>
            <code proglang="perl">not $infile_aln</code>
            <code proglang="python">not infile_aln</code>
          </ctrl>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>Input</xhtml:legend>
              <xhtml:textarea name="infile_seq" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="1,n" data-formats="EMBL FASTA GCG GENBANK IG NBRF CODATA RAW SWISSPROT " data-parametername="infile_seq">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Sequence)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
        <parameter ismandatory="1" issimple="1">
          <name>convert_seq</name>
          <prompt lang="en">Convert sequence into sequence format (-c)</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <vdef>
            <value>null</value>
          </vdef>
          <vlist>
            <velem undef="1">
              <value>null</value>
              <label>Choose the output format</label>
            </velem>
            <velem>
              <value>EMBL</value>
              <label>EMBL</label>
            </velem>
            <velem>
              <value>FASTA</value>
              <label>FASTA</label>
            </velem>
            <velem>
              <value>GCG</value>
              <label>GCG</label>
            </velem>
            <velem>
              <value>GDE</value>
              <label>GDE</label>
            </velem>
            <velem>
              <value>GENBANK</value>
              <label>GENBANK</label>
            </velem>
            <velem>
              <value>IG</value>
              <label>IG</label>
            </velem>
            <velem>
              <value>NBRF</value>
              <label>NBRF</label>
            </velem>
            <velem>
              <value>CODATA</value>
              <label>CODATA</label>
            </velem>
            <velem>
              <value>RAW</value>
              <label>RAW</label>
            </velem>
            <velem>
              <value>SWISSPROT</value>
              <label>SWISSPROT</label>
            </velem>
          </vlist>
          <format>
            <code proglang="perl">(defined $value) ? " -S -c $value" : ""</code>
            <code proglang="python">("", " -S -c "+str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Convert sequence into sequence format (-c)<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="convert_seq">
                <xhtml:option value="null" selected="selected">Choose the output format</xhtml:option>
                <xhtml:option value="EMBL">EMBL</xhtml:option>
                <xhtml:option value="FASTA">FASTA</xhtml:option>
                <xhtml:option value="GCG">GCG</xhtml:option>
                <xhtml:option value="GDE">GDE</xhtml:option>
                <xhtml:option value="GENBANK">GENBANK</xhtml:option>
                <xhtml:option value="IG">IG</xhtml:option>
                <xhtml:option value="NBRF">NBRF</xhtml:option>
                <xhtml:option value="CODATA">CODATA</xhtml:option>
                <xhtml:option value="RAW">RAW</xhtml:option>
                <xhtml:option value="SWISSPROT">SWISSPROT</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="convert_seq"/>
          </interface>
        </parameter>
        <parameter isstdout="1">
          <name>seq_outfile</name>
          <prompt>Sequence(s) file</prompt>
          <type>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>
              <ref param="convert_seq"/>
            </dataFormat>
            <card>1,n</card>
          </type>
          <precond>
            <code proglang="perl">defined $infile_seq</code>
            <code proglang="python">infile_seq is not None</code>
          </precond>
          <filenames>
            <code proglang="perl">"squizz_convert.out"</code>
            <code proglang="python">"squizz_convert.out"</code>
          </filenames>
          <interface generated="true" type="job_output">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="1,n" data-formats="&#10;               " data-parametername="seq_outfile">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Sequence)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>aln</name>
      <prompt lang="en">Alignment section</prompt>
      <precond>
        <code proglang="perl">not $infile_seq or ($infile_seq and $infile_aln)</code>
        <code proglang="python">not infile_seq or (infile_seq and infile_aln)</code>
      </precond>
      <parameters>
        <parameter ismandatory="1" issimple="1">
          <name>infile_aln</name>
          <prompt lang="en">Input</prompt>
          <type>
            <datatype>
              <class>Alignment</class>
            </datatype>
            <dataFormat>CLUSTAL</dataFormat>
            <dataFormat>PHYLIPI</dataFormat>
            <dataFormat>PHYLIPS</dataFormat>
            <dataFormat>MSF</dataFormat>
            <dataFormat>NEXUS</dataFormat>
            <dataFormat>STOCKHOLM</dataFormat>
            <dataFormat>FASTA</dataFormat>
            <dataFormat>MEGA</dataFormat>
          </type>
          <format>
            <code proglang="perl">" $value"</code>
            <code proglang="python">" "+str(value)</code>
          </format>
          <ctrl>
            <message>
              <text lang="en">Can not handle both Sequence and Alignment at the same time</text>
            </message>
            <code proglang="perl">not $infile_seq</code>
            <code proglang="python">not infile_seq</code>
          </ctrl>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>Input</xhtml:legend>
              <xhtml:textarea name="infile_aln" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Alignment" data-datatype-superclass="" data-biotype="" data-card="" data-formats="CLUSTAL PHYLIPI PHYLIPS MSF NEXUS STOCKHOLM FASTA MEGA " data-parametername="infile_aln">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Alignment)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
        <parameter ismandatory="1" issimple="1">
          <name>convert_aln</name>
          <prompt lang="en">Convert alignment into alignment format (-c)</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <precond>
            <code proglang="perl">not defined $convert_seq2 or (defined $convert_aln and defined $convert_seq2)</code>
            <code proglang="python">convert_seq2 is None or (convert_aln is not None and convert_seq2 is not None)</code>
          </precond>
          <vdef>
            <value>null</value>
          </vdef>
          <vlist>
            <velem undef="1">
              <value>null</value>
              <label>Choose the output format</label>
            </velem>
            <velem>
              <value>CLUSTAL</value>
              <label>CLUSTAL</label>
            </velem>
            <velem>
              <value>FASTA</value>
              <label>FASTA</label>
            </velem>
            <velem>
              <value>MEGA</value>
              <label>MEGA</label>
            </velem>
            <velem>
              <value>MSF</value>
              <label>MSF</label>
            </velem>
            <velem>
              <value>NEXUS</value>
              <label>NEXUS</label>
            </velem>
            <velem>
              <value>PHYLIPI</value>
              <label>PHYLIP Interleaved</label>
            </velem>
            <velem>
              <value>PHYLIPS</value>
              <label>PHYLIP Sequential</label>
            </velem>
            <velem>
              <value>STOCKHOLM</value>
              <label>STOCKHOLM</label>
            </velem>
          </vlist>
          <format>
            <code proglang="perl">(defined $value) ? "-A -c $value" : ""</code>
            <code proglang="python">("", " -A -c "+str(value))[value is not None]</code>
          </format>
          <ctrl>
            <message>
              <text lang="en">Cannot convert to both Sequence and Alignment at the same time</text>
            </message>
            <code proglang="perl">defined $convert_aln and not defined $convert_seq2</code>
            <code proglang="python">convert_aln is not None and convert_seq2 is None</code>
          </ctrl>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Convert alignment into alignment format (-c)<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="convert_aln">
                <xhtml:option value="null" selected="selected">Choose the output format</xhtml:option>
                <xhtml:option value="CLUSTAL">CLUSTAL</xhtml:option>
                <xhtml:option value="FASTA">FASTA</xhtml:option>
                <xhtml:option value="MEGA">MEGA</xhtml:option>
                <xhtml:option value="MSF">MSF</xhtml:option>
                <xhtml:option value="NEXUS">NEXUS</xhtml:option>
                <xhtml:option value="PHYLIPI">PHYLIP Interleaved</xhtml:option>
                <xhtml:option value="PHYLIPS">PHYLIP Sequential</xhtml:option>
                <xhtml:option value="STOCKHOLM">STOCKHOLM</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9; conditional" title="This parameter is mandatory under certain conditions." data-issimple="true" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="convert_aln"/>
          </interface>
        </parameter>
        <parameter ismandatory="1" issimple="1">
          <name>convert_seq2</name>
          <prompt lang="en">Convert alignment into sequence format (-c)</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <precond>
            <code proglang="perl">not defined $convert_aln or (defined $convert_aln and defined $convert_seq2)</code>
            <code proglang="python">convert_aln is None or (convert_aln is not None and convert_seq2 is not None)</code>
          </precond>
          <vdef>
            <value>null</value>
          </vdef>
          <vlist>
            <velem undef="1">
              <value>null</value>
              <label>Choose the output format</label>
            </velem>
            <velem>
              <value>EMBL</value>
