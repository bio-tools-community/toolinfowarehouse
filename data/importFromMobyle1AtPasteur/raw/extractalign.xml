            <dataFormat>MULTIPLE</dataFormat>
            <dataFormat>SIMPLE</dataFormat>
            <dataFormat>MATCH</dataFormat>
            <card>1,n</card>
          </type>
          <format>
            <code proglang="python">("", " -sequence=" + str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
