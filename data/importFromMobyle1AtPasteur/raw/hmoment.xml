<program>
  <head>
    <name>hmoment</name>
    <package>
      <name>EMBOSS</name>
      <version>6.3.1</version>
      <doc>
        <title>EMBOSS</title>
        <description>
          <text lang="en">European Molecular Biology Open Software Suite</text>
        </description>
        <authors>Rice,P. Longden,I. and Bleasby, A.</authors>
        <reference>EMBOSS: The European Molecular Biology Open Software Suite (2000)  Rice,P. Longden,I. and Bleasby, A. Trends in Genetics 16, (6) pp276--277</reference>
        <sourcelink>http://emboss.sourceforge.net/download</sourcelink>
        <homepagelink>http://emboss.sourceforge.net</homepagelink>
      </doc>
    </package>
    <doc>
      <title>hmoment</title>
      <description>
        <text lang="en">Calculate and plot hydrophobic moment for protein sequence(s)</text>
      </description>
      <doclink>http://bioweb2.pasteur.fr/docs/EMBOSS/hmoment.html</doclink>
      <doclink>http://emboss.sourceforge.net/docs/themes</doclink>
    </doc>
    <command>hmoment</command>
    <category>sequence:protein:2D_structure</category>
    <category>structure:2D_structure</category>
    <interface type="form" generated="true">
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Input section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <fieldset class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="Protein " data-card="1,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_seqall">
            <legend>seqall option</legend>
            <xhtml:textarea name="e_seqall" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_additional">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Additional section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="10" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_window">Window<xhtml:input name="e_window" type="text" value="10"/>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="100" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_aangle">Alpha helix angle (degrees)<xhtml:input name="e_aangle" type="text" value="100"/>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="160" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_bangle">Beta sheet angle (degrees)<xhtml:input name="e_bangle" type="text" value="160"/>
          </label>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_advanced">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Advanced section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0.35" data-datatype="Float" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_baseline">Graph marker line<xhtml:input name="e_baseline" type="text" value="0.35"/>
          </label>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Output section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_plot">Produce graphic<xhtml:select name="e_plot">
              <xhtml:option value="1">Yes</xhtml:option>
              <xhtml:option value="0" selected="selected">No</xhtml:option>
            </xhtml:select>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_double">Plot two graphs<xhtml:select name="e_double">
              <xhtml:option value="1">Yes</xhtml:option>
              <xhtml:option value="0" selected="selected">No</xhtml:option>
            </xhtml:select>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="png" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_graph">Choose the e_graph output format<xhtml:select name="e_graph">
              <xhtml:option value="png" selected="selected">Png</xhtml:option>
              <xhtml:option value="gif">Gif</xhtml:option>
              <xhtml:option value="cps">Cps</xhtml:option>
              <xhtml:option value="ps">Ps</xhtml:option>
              <xhtml:option value="meta">Meta</xhtml:option>
              <xhtml:option value="data">Data</xhtml:option>
            </xhtml:select>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="hmoment_xygraph" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="xy_goutfile">Name of the output graph<xhtml:input name="xy_goutfile" type="text" value="hmoment_xygraph"/>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="hmoment.e_outfile" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile">Name of the output file (e_outfile)<xhtml:input name="e_outfile" type="text" value="hmoment.e_outfile"/>
          </label>
        </xhtml:div>
      </fieldset>
    </interface>
    <interface type="job_input" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <xhtml:legend>Input section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_seqall" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="Protein " data-card="1,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF ">
            <xhtml:i>
				(Protein Sequence)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_additional">
        <xhtml:legend>Additional section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_window" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="10" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_aangle" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="100" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_bangle" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="160" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_advanced">
        <xhtml:legend>Advanced section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_baseline" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0.35" data-datatype="Float" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_plot" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_double" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_graph" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="png" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_goutfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="hmoment_xygraph" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="hmoment.e_outfile" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="auto" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
    </interface>
    <interface type="job_output" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_png" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Picture" data-datatype-superclass="Binary" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_gif" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Picture" data-datatype-superclass="Binary" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_ps" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="PostScript" data-datatype-superclass="Binary" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_meta" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Picture" data-datatype-superclass="Binary" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_data" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Text" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
            <xhtml:i>
				(Text)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outfile_out" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="HmomentReport" data-datatype-superclass="Report" data-biotype="" data-card="" data-formats="">
            <xhtml:i>
				(HmomentReport)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stdout" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stderr" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
    </interface>
  </head>
  <parameters>
    <paragraph>
      <name>e_input</name>
      <prompt lang="en">Input section</prompt>
      <parameters>
        <parameter issimple="1" ismandatory="1">
          <name>e_seqall</name>
          <prompt lang="en">seqall option</prompt>
          <type>
            <biotype>Protein</biotype>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>EMBL</dataFormat>
            <dataFormat>FASTA</dataFormat>
            <dataFormat>GCG</dataFormat>
            <dataFormat>GENBANK</dataFormat>
            <dataFormat>NBRF</dataFormat>
            <dataFormat>CODATA</dataFormat>
            <dataFormat>RAW</dataFormat>
            <dataFormat>SWISSPROT</dataFormat>
            <dataFormat>GFF</dataFormat>
            <card>1,n</card>
          </type>
          <format>
            <code proglang="python">("", " -seqall=" + str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>seqall option</xhtml:legend>
              <xhtml:textarea name="e_seqall" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="Protein " data-card="1,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_seqall">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Protein Sequence)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_additional</name>
      <prompt lang="en">Additional section</prompt>
      <parameters>
        <parameter>
          <name>e_window</name>
          <prompt lang="en">Window</prompt>
          <type>
            <datatype>
              <class>Integer</class>
            </datatype>
          </type>
          <vdef>
            <value>10</value>
          </vdef>
          <format>
            <code proglang="python">("", " -window=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Window<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_window" type="text" value="10"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="10" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_window"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_aangle</name>
          <prompt lang="en">Alpha helix angle (degrees)</prompt>
          <type>
            <datatype>
              <class>Integer</class>
            </datatype>
          </type>
          <vdef>
            <value>100</value>
          </vdef>
          <format>
            <code proglang="python">("", " -aangle=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>3</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Alpha helix angle (degrees)<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_aangle" type="text" value="100"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="100" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_aangle"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_bangle</name>
          <prompt lang="en">Beta sheet angle (degrees)</prompt>
          <type>
            <datatype>
              <class>Integer</class>
            </datatype>
          </type>
          <vdef>
            <value>160</value>
          </vdef>
          <format>
            <code proglang="python">("", " -bangle=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>4</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Beta sheet angle (degrees)<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_bangle" type="text" value="160"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="160" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_bangle"/>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_advanced</name>
      <prompt lang="en">Advanced section</prompt>
      <parameters>
        <parameter>
          <name>e_baseline</name>
          <prompt lang="en">Graph marker line</prompt>
          <type>
            <datatype>
              <class>Float</class>
            </datatype>
          </type>
          <vdef>
            <value>0.35</value>
          </vdef>
          <format>
            <code proglang="python">("", " -baseline=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>5</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Graph marker line<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_baseline" type="text" value="0.35"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0.35" data-datatype="Float" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_baseline"/>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_output</name>
      <prompt lang="en">Output section</prompt>
      <parameters>
        <parameter>
          <name>e_plot</name>
          <prompt lang="en">Produce graphic</prompt>
          <type>
            <datatype>
              <class>Boolean</class>
            </datatype>
          </type>
          <vdef>
            <value>0</value>
          </vdef>
          <format>
            <code proglang="python">("", " -plot")[ bool(value) ]</code>
          </format>
          <argpos>6</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Produce graphic<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_plot">
                <xhtml:option value="1">Yes</xhtml:option>
                <xhtml:option value="0" selected="selected">No</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_plot"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_double</name>
          <prompt lang="en">Plot two graphs</prompt>
          <type>
            <datatype>
              <class>Boolean</class>
            </datatype>
          </type>
          <vdef>
            <value>0</value>
          </vdef>
          <format>
            <code proglang="python">("", " -double")[ bool(value) ]</code>
          </format>
          <argpos>7</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Plot two graphs<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_double">
                <xhtml:option value="1">Yes</xhtml:option>
                <xhtml:option value="0" selected="selected">No</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_double"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_graph</name>
          <prompt lang="en">Choose the e_graph output format</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <precond>
            <code proglang="python">e_plot</code>
          </precond>
          <vdef>
            <value>png</value>
          </vdef>
          <vlist>
            <velem>
              <value>png</value>
              <label>Png</label>
            </velem>
            <velem>
              <value>gif</value>
              <label>Gif</label>
            </velem>
            <velem>
              <value>cps</value>
              <label>Cps</label>
            </velem>
            <velem>
              <value>ps</value>
              <label>Ps</label>
            </velem>
