  <parameters>
    <paragraph>
      <name>e_input</name>
      <prompt lang="en">Input section</prompt>
      <parameters>
        <parameter issimple="1" ismandatory="1">
          <name>e_sequences</name>
          <prompt lang="en">sequences option</prompt>
          <type>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>EMBL</dataFormat>
            <dataFormat>FASTA</dataFormat>
            <dataFormat>GCG</dataFormat>
            <dataFormat>GENBANK</dataFormat>
            <dataFormat>NBRF</dataFormat>
            <dataFormat>CODATA</dataFormat>
            <dataFormat>RAW</dataFormat>
            <dataFormat>SWISSPROT</dataFormat>
            <dataFormat>GFF</dataFormat>
            <card>2,n</card>
          </type>
          <format>
            <code proglang="python">("", " -sequences=" + str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
          <comment>
            <text lang="en">File containing a sequence alignment</text>
          </comment>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>sequences option</xhtml:legend>
              <xhtml:textarea name="e_sequences" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="2,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_sequences">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Sequence)
			</xhtml:i>
              <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117725762816::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
              <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117725762816::comment" class="commentText" style="display:none" mode="ajaxTarget">
                <xhtml:div>File containing a sequence alignment</xhtml:div>
              </xhtml:div>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_required</name>
      <prompt lang="en">Required section</prompt>
      <parameters>
        <parameter issimple="1" ismandatory="1">
          <name>e_wordsize</name>
          <prompt lang="en">Word size (value greater than or equal to 2)</prompt>
          <type>
            <datatype>
              <class>Integer</class>
            </datatype>
          </type>
          <vdef>
            <value>6</value>
          </vdef>
          <format>
            <code proglang="python">("", " -wordsize=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <ctrl>
            <message>
              <text lang="en">Value greater than or equal to 2 is required</text>
            </message>
            <code proglang="python">value &gt;= 2</code>
          </ctrl>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Word size (value greater than or equal to 2)<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_wordsize" type="text" value="6"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="6" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_wordsize"/>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_output</name>
      <prompt lang="en">Output section</prompt>
      <parameters>
        <parameter>
          <name>e_gap</name>
          <prompt lang="en">Gap (in residues) between dotplots (value greater than or equal to 0)</prompt>
          <type>
            <datatype>
              <class>Integer</class>
            </datatype>
          </type>
          <vdef>
            <value>10</value>
          </vdef>
          <format>
            <code proglang="python">("", " -gap=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <ctrl>
            <message>
              <text lang="en">Value greater than or equal to 0 is required</text>
            </message>
            <code proglang="python">value &gt;= 0</code>
          </ctrl>
          <argpos>3</argpos>
          <comment>
            <text lang="en">This specifies the size of the gap that is used to separate the individual dotplots in the display. The size is measured in  residues, as displayed in the output.</text>
          </comment>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Gap (in residues) between dotplots (value greater than or equal to 0)<xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117725106064::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
              <xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_gap" type="text" value="10"/>
              <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117725106064::comment" class="commentText" style="display:none" mode="ajaxTarget">
                <xhtml:div>This specifies the size of the gap that is used to separate the individual dotplots in the display. The size is measured in  residues, as displayed in the output.</xhtml:div>
              </xhtml:div>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="10" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_gap">
              <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117725106064::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
              <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117725106064::comment" class="commentText" style="display:none" mode="ajaxTarget">
                <xhtml:div>This specifies the size of the gap that is used to separate the individual dotplots in the display. The size is measured in  residues, as displayed in the output.</xhtml:div>
              </xhtml:div>
            </div>
          </interface>
        </parameter>
        <parameter>
          <name>e_boxit</name>
          <prompt lang="en">Draw a box around each dotplot</prompt>
          <type>
            <datatype>
              <class>Boolean</class>
            </datatype>
          </type>
          <vdef>
            <value>1</value>
          </vdef>
          <format>
            <code proglang="python">(" -noboxit", "")[ bool(value) ]</code>
          </format>
          <argpos>4</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Draw a box around each dotplot<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_boxit">
                <xhtml:option value="1" selected="selected">Yes</xhtml:option>
                <xhtml:option value="0">No</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="1" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_boxit"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_dumpfeat</name>
          <prompt lang="en">Dump all matches as feature files</prompt>
          <type>
            <datatype>
              <class>Boolean</class>
            </datatype>
          </type>
          <vdef>
            <value>0</value>
          </vdef>
          <format>
            <code proglang="python">("", " -dumpfeat")[ bool(value) ]</code>
          </format>
