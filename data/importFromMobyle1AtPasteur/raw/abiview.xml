  <parameters>
    <paragraph>
      <name>e_input</name>
      <prompt lang="en">Input section</prompt>
      <parameters>
        <parameter issimple="1" ismandatory="1">
          <name>e_infile</name>
          <prompt lang="en">Abi sequencing trace file</prompt>
          <type>
            <datatype>
              <class>Binary</class>
            </datatype>
          </type>
          <format>
            <code proglang="python">("", " -infile=" + str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>Abi sequencing trace file</xhtml:legend>
              <xhtml:input name="e_infile" type="file" value=""/>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Binary" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_infile"/>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_output</name>
      <prompt lang="en">Output section</prompt>
      <parameters>
        <parameter>
          <name>e_outseq</name>
          <prompt lang="en">Name of the output sequence file (e_outseq)</prompt>
          <type>
            <biotype>DNA</biotype>
            <datatype>
              <class>Filename</class>
            </datatype>
          </type>
          <vdef>
            <value>abiview.e_outseq</value>
          </vdef>
          <format>
            <code proglang="python">("" , " -outseq=" + str(value))[value is not None]</code>
          </format>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Name of the output sequence file (e_outseq)<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_outseq" type="text" value="abiview.e_outseq"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="abiview.e_outseq" data-datatype="Filename" data-datatype-superclass="" data-biotype="DNA " data-card="" data-formats="" data-parametername="e_outseq"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_osformat_outseq</name>
          <prompt lang="en">Choose the sequence output format</prompt>
          <type>
            <biotype>DNA</biotype>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <vdef>
            <value>FASTA</value>
          </vdef>
          <vlist>
            <velem>
              <value>EMBL</value>
              <label>Embl</label>
            </velem>
            <velem>
              <value>FASTA</value>
              <label>Fasta</label>
            </velem>
            <velem>
              <value>GCG</value>
              <label>Gcg</label>
            </velem>
            <velem>
              <value>GENBANK</value>
              <label>Genbank</label>
            </velem>
            <velem>
              <value>NBRF</value>
              <label>Nbrf</label>
            </velem>
            <velem>
              <value>CODATA</value>
              <label>Codata</label>
            </velem>
            <velem>
              <value>RAW</value>
              <label>Raw</label>
            </velem>
            <velem>
              <value>SWISSPROT</value>
              <label>Swissprot</label>
            </velem>
            <velem>
              <value>GFF</value>
              <label>Gff</label>
            </velem>
          </vlist>
          <format>
            <code proglang="python">("", " -osformat=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>3</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Choose the sequence output format<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_osformat_outseq">
                <xhtml:option value="EMBL">Embl</xhtml:option>
                <xhtml:option value="FASTA" selected="selected">Fasta</xhtml:option>
                <xhtml:option value="GCG">Gcg</xhtml:option>
                <xhtml:option value="GENBANK">Genbank</xhtml:option>
                <xhtml:option value="NBRF">Nbrf</xhtml:option>
                <xhtml:option value="CODATA">Codata</xhtml:option>
                <xhtml:option value="RAW">Raw</xhtml:option>
                <xhtml:option value="SWISSPROT">Swissprot</xhtml:option>
                <xhtml:option value="GFF">Gff</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="FASTA" data-datatype="Choice" data-datatype-superclass="" data-biotype="DNA " data-card="" data-formats="" data-parametername="e_osformat_outseq"/>
          </interface>
        </parameter>
        <parameter isout="1">
          <name>e_outseq_out</name>
          <prompt lang="en">outseq_out option</prompt>
          <type>
            <biotype>DNA</biotype>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>
              <ref param="e_osformat_outseq">
              </ref>
            </dataFormat>
          </type>
          <filenames>
            <code proglang="python">e_outseq</code>
          </filenames>
          <interface generated="true" type="job_output">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="" data-formats="&#10;               " data-parametername="e_outseq_out">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(DNA Sequence)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
        <parameter issimple="1" ismandatory="1">
          <name>e_graph</name>
          <prompt lang="en">Choose the e_graph output format</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <vdef>
            <value>png</value>
          </vdef>
          <vlist>
            <velem>
              <value>png</value>
              <label>Png</label>
            </velem>
            <velem>
              <value>gif</value>
              <label>Gif</label>
            </velem>
            <velem>
              <value>cps</value>
              <label>Cps</label>
            </velem>
            <velem>
              <value>ps</value>
              <label>Ps</label>
            </velem>
            <velem>
              <value>meta</value>
              <label>Meta</label>
            </velem>
            <velem>
              <value>data</value>
              <label>Data</label>
            </velem>
          </vlist>
          <format>
            <code proglang="python">(" -graph=" + str(vdef), " -graph=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>4</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Choose the e_graph output format<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_graph">
                <xhtml:option value="png" selected="selected">Png</xhtml:option>
                <xhtml:option value="gif">Gif</xhtml:option>
                <xhtml:option value="cps">Cps</xhtml:option>
                <xhtml:option value="ps">Ps</xhtml:option>
                <xhtml:option value="meta">Meta</xhtml:option>
                <xhtml:option value="data">Data</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="png" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_graph"/>
          </interface>
