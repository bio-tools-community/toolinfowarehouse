        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outseq" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="abiview.e_outseq" data-datatype="Filename" data-datatype-superclass="" data-biotype="DNA " data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_osformat_outseq" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="FASTA" data-datatype="Choice" data-datatype-superclass="" data-biotype="DNA " data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_graph" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="png" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_goutfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="abiview_xygraph" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_startbase" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_endbase" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
            <xhtml:a href="#idm140117711684512::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
            <xhtml:div id="idm140117711684512::comment" class="commentText" style="display:none" mode="ajaxTarget">
              <xhtml:div>Last sequence base to report or display. If the default is set to zero then the value of this is taken as the maximum number  of bases.</xhtml:div>
            </xhtml:div>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_separate" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_yticks" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_sequence" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="1" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_window" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="40" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_bases" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="GATC" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="auto" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
    </interface>
    <interface type="job_output" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outseq_out" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="" data-formats="&#10;               ">
            <xhtml:i>
				(DNA Sequence)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_png" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Picture" data-datatype-superclass="Binary" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="xy_outgraph_gif" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Picture" data-datatype-superclass="Binary" data-biotype="" data-card="" data-formats=""/>
