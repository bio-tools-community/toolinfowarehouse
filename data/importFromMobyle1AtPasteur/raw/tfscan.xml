              <xhtml:option value="F">Fungi</xhtml:option>
              <xhtml:option value="I">Insect</xhtml:option>
              <xhtml:option value="P">Plant</xhtml:option>
              <xhtml:option value="V" selected="selected">Vertebrate</xhtml:option>
              <xhtml:option value="O">Other</xhtml:option>
              <xhtml:option value="C">Custom</xhtml:option>
            </xhtml:select>
          </label>
          <fieldset class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="TransfacData" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="" data-parametername="e_custom">
            <legend>Transfac database data file (optional)</legend>
            <xhtml:textarea name="e_custom" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_required">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Required section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="0" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_mismatch">Number of mismatches (value greater than or equal to 0)<xhtml:input name="e_mismatch" type="text" value="0"/>
          </label>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_additional">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Additional section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="1" data-datatype="Integer" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_minlength">Display matches equal to or above this length (value greater than or equal to 1)<xhtml:input name="e_minlength" type="text" value="1"/>
          </label>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Output section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="tfscan.report" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile">Name of the report file<xhtml:input name="e_outfile" type="text" value="tfscan.report"/>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="SEQTABLE" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_rformat_outfile">Choose the report output format<xhtml:select name="e_rformat_outfile">
              <xhtml:option value="DASGFF">Dasgff</xhtml:option>
              <xhtml:option value="DBMOTIF">Dbmotif</xhtml:option>
              <xhtml:option value="DIFFSEQ">Diffseq</xhtml:option>
              <xhtml:option value="EMBL">Embl</xhtml:option>
              <xhtml:option value="EXCEL">Excel</xhtml:option>
              <xhtml:option value="FEATTABLE">Feattable</xhtml:option>
              <xhtml:option value="GENBANK">Genbank</xhtml:option>
              <xhtml:option value="GFF">Gff</xhtml:option>
              <xhtml:option value="LISTFILE">Listfile</xhtml:option>
              <xhtml:option value="MOTIF">Motif</xhtml:option>
              <xhtml:option value="NAMETABLE">Nametable</xhtml:option>
              <xhtml:option value="CODATA">Codata</xhtml:option>
              <xhtml:option value="REGIONS">Regions</xhtml:option>
              <xhtml:option value="SEQTABLE" selected="selected">Seqtable</xhtml:option>
              <xhtml:option value="SIMPLE">Simple</xhtml:option>
              <xhtml:option value="SRS">Srs</xhtml:option>
              <xhtml:option value="SWISS">Swiss</xhtml:option>
              <xhtml:option value="TABLE">Table</xhtml:option>
              <xhtml:option value="TAGSEQ">Tagseq</xhtml:option>
            </xhtml:select>
          </label>
        </xhtml:div>
      </fieldset>
    </interface>
    <interface type="job_input" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <xhtml:legend>Input section</xhtml:legend>
