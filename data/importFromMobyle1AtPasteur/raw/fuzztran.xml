        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="1" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_frame">Translation frames<xhtml:select name="e_frame">
              <xhtml:option value="1" selected="selected">1</xhtml:option>
              <xhtml:option value="2">2</xhtml:option>
              <xhtml:option value="3">3</xhtml:option>
              <xhtml:option value="F">Forward three frames</xhtml:option>
              <xhtml:option value="-1">-1</xhtml:option>
              <xhtml:option value="-2">-2</xhtml:option>
