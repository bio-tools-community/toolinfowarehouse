<program>
  <head>
    <name>btwisted</name>
    <package>
      <name>EMBOSS</name>
      <version>6.3.1</version>
      <doc>
        <title>EMBOSS</title>
        <description>
          <text lang="en">European Molecular Biology Open Software Suite</text>
        </description>
        <authors>Rice,P. Longden,I. and Bleasby, A.</authors>
        <reference>EMBOSS: The European Molecular Biology Open Software Suite (2000)  Rice,P. Longden,I. and Bleasby, A. Trends in Genetics 16, (6) pp276--277</reference>
        <sourcelink>http://emboss.sourceforge.net/download</sourcelink>
        <homepagelink>http://emboss.sourceforge.net</homepagelink>
      </doc>
    </package>
    <doc>
      <title>btwisted</title>
      <description>
        <text lang="en">Calculate the twisting in a B-DNA sequence</text>
      </description>
      <doclink>http://bioweb2.pasteur.fr/docs/EMBOSS/btwisted.html</doclink>
      <doclink>http://emboss.sourceforge.net/docs/themes</doclink>
    </doc>
    <command>btwisted</command>
    <category>sequence:nucleic:composition</category>
    <interface type="form" generated="true">
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Input section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <fieldset class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="1,1" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_sequence">
            <legend>sequence option</legend>
            <xhtml:textarea name="e_sequence" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
          <fieldset class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BasePairTwistAngles" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="" data-parametername="e_angledata">
            <legend>Dna base pair twist angle data file</legend>
            <xhtml:textarea name="e_angledata" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
          <fieldset class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BasePairStackingEnergies" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="" data-parametername="e_energydata">
            <legend>Dna base pair stacking energies data file</legend>
            <xhtml:textarea name="e_energydata" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Output section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="btwisted.e_outfile" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile">Name of the output file (e_outfile)<xhtml:input name="e_outfile" type="text" value="btwisted.e_outfile"/>
          </label>
        </xhtml:div>
      </fieldset>
    </interface>
    <interface type="job_input" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <xhtml:legend>Input section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_sequence" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="1,1" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF ">
            <xhtml:i>
				(DNA Sequence)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_angledata" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BasePairTwistAngles" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="">
            <xhtml:i>
				(BasePairTwistAngles)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_energydata" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BasePairStackingEnergies" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="">
            <xhtml:i>
				(BasePairStackingEnergies)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="btwisted.e_outfile" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="auto" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
    </interface>
    <interface type="job_output" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outfile_out" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BtwistedReport" data-datatype-superclass="Report" data-biotype="" data-card="" data-formats="">
            <xhtml:i>
				(BtwistedReport)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stdout" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stderr" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
    </interface>
  </head>
  <parameters>
    <paragraph>
      <name>e_input</name>
      <prompt lang="en">Input section</prompt>
      <parameters>
        <parameter issimple="1" ismandatory="1">
          <name>e_sequence</name>
          <prompt lang="en">sequence option</prompt>
          <type>
            <biotype>DNA</biotype>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>EMBL</dataFormat>
            <dataFormat>FASTA</dataFormat>
            <dataFormat>GCG</dataFormat>
            <dataFormat>GENBANK</dataFormat>
            <dataFormat>NBRF</dataFormat>
            <dataFormat>CODATA</dataFormat>
            <dataFormat>RAW</dataFormat>
            <dataFormat>SWISSPROT</dataFormat>
            <dataFormat>GFF</dataFormat>
            <card>1,1</card>
          </type>
          <format>
            <code proglang="python">("", " -sequence=" + str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>sequence option</xhtml:legend>
              <xhtml:textarea name="e_sequence" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="1,1" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_sequence">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(DNA Sequence)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
        <parameter>
          <name>e_angledata</name>
          <prompt lang="en">Dna base pair twist angle data file</prompt>
          <type>
            <datatype>
              <class>BasePairTwistAngles</class>
              <superclass>AbstractText</superclass>
            </datatype>
          </type>
          <format>
            <code proglang="python">("", " -angledata=" + str(value))[value is not None ]</code>
          </format>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>Dna base pair twist angle data file</xhtml:legend>
              <xhtml:textarea name="e_angledata" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BasePairTwistAngles" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="" data-parametername="e_angledata">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(BasePairTwistAngles)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
        <parameter>
          <name>e_energydata</name>
          <prompt lang="en">Dna base pair stacking energies data file</prompt>
          <type>
            <datatype>
              <class>BasePairStackingEnergies</class>
              <superclass>AbstractText</superclass>
            </datatype>
          </type>
          <format>
            <code proglang="python">("", " -energydata=" + str(value))[value is not None ]</code>
          </format>
          <argpos>3</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>Dna base pair stacking energies data file</xhtml:legend>
              <xhtml:textarea name="e_energydata" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BasePairStackingEnergies" data-datatype-superclass="AbstractText" data-biotype="" data-card="" data-formats="" data-parametername="e_energydata">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(BasePairStackingEnergies)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_output</name>
      <prompt lang="en">Output section</prompt>
      <parameters>
        <parameter>
          <name>e_outfile</name>
          <prompt lang="en">Name of the output file (e_outfile)</prompt>
          <type>
            <datatype>
              <class>Filename</class>
            </datatype>
          </type>
          <vdef>
            <value>btwisted.e_outfile</value>
          </vdef>
          <format>
            <code proglang="python">("" , " -outfile=" + str(value))[value is not None]</code>
          </format>
          <argpos>4</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Name of the output file (e_outfile)<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_outfile" type="text" value="btwisted.e_outfile"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="btwisted.e_outfile" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile"/>
          </interface>
        </parameter>
        <parameter isout="1">
          <name>e_outfile_out</name>
          <prompt lang="en">outfile_out option</prompt>
          <type>
            <datatype>
              <class>BtwistedReport</class>
              <superclass>Report</superclass>
            </datatype>
          </type>
          <filenames>
            <code proglang="python">e_outfile</code>
          </filenames>
          <interface generated="true" type="job_output">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="BtwistedReport" data-datatype-superclass="Report" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile_out">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(BtwistedReport)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <parameter ishidden="1">
      <name>auto</name>
      <prompt lang="en">Turn off any prompting</prompt>
      <type>
        <datatype>
          <class>String</class>
        </datatype>
      </type>
      <format>
        <code proglang="python">" -auto -stdout"</code>
      </format>
      <argpos>5</argpos>
      <interface generated="true" type="job_input">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="auto"/>
      </interface>
    </parameter>
    <parameter isstdout="true">
      <name>stdout</name>
      <prompt>Standard output</prompt>
      <type>
        <datatype>
          <class>Report</class>
        </datatype>
      </type>
      <interface type="job_output" generated="true">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="stdout">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
        </div>
      </interface>
      <filenames>
        <code proglang="perl">"btwisted.out"</code>
        <code proglang="python">"btwisted.out"</code>
      </filenames>
    </parameter>
    <parameter isout="true">
      <name>stderr</name>
      <prompt>Standard error</prompt>
      <type>
        <datatype>
          <class>Report</class>
        </datatype>
      </type>
      <interface type="job_output" generated="true">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="stderr">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
        </div>
      </interface>
      <filenames>
        <code proglang="perl">"btwisted.err"</code>
        <code proglang="python">"btwisted.err"</code>
      </filenames>
    </parameter>
  </parameters>
</program>

