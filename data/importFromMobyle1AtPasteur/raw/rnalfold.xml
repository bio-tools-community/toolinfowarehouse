            <datatype>
              <class>String</class>
            </datatype>
          </type>
          <format>
            <code proglang="perl">(defined $value)? " -nsp $value" : "" </code>
            <code proglang="python">( ""  , " -nsp " + str(value) )[ value is not None ]</code>
          </format>
