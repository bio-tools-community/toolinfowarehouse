<program>
  <head>
    <name>nw_cat</name>
    <version>GNU 7.4</version>
    <doc>
      <title>nw_cat</title>
      <description>
        <text lang="en">concatenate phylogenetic tree files</text>
      </description>
    </doc>
    <command>cat</command>
    <category>phylogeny:others</category>
    <interface type="form" generated="true">
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK " data-parametername="input">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">First treeset<xhtml:a href="#idm140117725215744::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
        </legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117725215744::comment" class="commentText" style="display:none" mode="ajaxTarget">enter a phylogenetic tree</xhtml:div>
        <xhtml:textarea xmlns:xhtml="http://www.w3.org/1999/xhtml" name="input" cols="60" rows="7"> </xhtml:textarea>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK " data-parametername="input_2">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Second treeset<xhtml:a href="#idm140117728296784::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
          <a href="#idm140117728296784::example" class="exampleLink" title="click to prefill with an example">[use example data]</a>
        </legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117728296784::comment" class="commentText" style="display:none" mode="ajaxTarget">type any kind of text<div class="example" id="idm140117728296784::example" data-forparameter="input_2">Example data <i>(click on <a>[use example data]</a> to load)</i>:
			<pre>example text</pre>
          </div>
        </xhtml:div>
        <div xmlns:xhtml="http://www.w3.org/1999/xhtml" class="example" id="idm140117728296784::example" data-forparameter="input_2" style="display:none">
          <pre>example text</pre>
        </div>
        <xhtml:textarea xmlns:xhtml="http://www.w3.org/1999/xhtml" name="input_2" cols="60" rows="7"> </xhtml:textarea>
      </fieldset>
    </interface>
    <interface type="job_input" generated="true">
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="input" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK ">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Tree)
			</xhtml:i>
        <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117725215744::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117725215744::comment" class="commentText" style="display:none" mode="ajaxTarget">enter a phylogenetic tree</xhtml:div>
      </div>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="input_2" class="parameter " title="" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK ">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Tree)
			</xhtml:i>
        <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117728296784::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117728296784::comment" class="commentText" style="display:none" mode="ajaxTarget">type any kind of text</xhtml:div>
      </div>
    </interface>
    <interface type="job_output" generated="true">
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="output" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK ">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Tree)
			</xhtml:i>
      </div>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stderr" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
    </interface>
  </head>
  <parameters>
    <parameter ismandatory="1" issimple="1">
      <name>input</name>
      <prompt lang="en">First treeset</prompt>
      <comment>enter a phylogenetic tree</comment>
      <type>
        <datatype>
          <class>Tree</class>
        </datatype>
        <dataFormat>NEWICK</dataFormat>
      </type>
      <format>
        <code proglang="perl">" $value"</code>
        <code proglang="python">" " + str(value)</code>
      </format>
      <argpos>1</argpos>
      <interface type="form" generated="true">
        <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <xhtml:legend>First treeset</xhtml:legend>
          <xhtml:textarea name="input" cols="60" rows="7"> </xhtml:textarea>
        </xhtml:fieldset>
      </interface>
      <interface generated="true" type="job_input">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK " data-parametername="input">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Tree)
			</xhtml:i>
          <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117725215744::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
          <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117725215744::comment" class="commentText" style="display:none" mode="ajaxTarget">enter a phylogenetic tree</xhtml:div>
        </div>
      </interface>
    </parameter>
    <parameter issimple="1">
      <name>input_2</name>
      <prompt lang="en">Second treeset</prompt>
      <comment>type any kind of text</comment>
      <example>example text</example>
      <type>
        <datatype>
          <class>Tree</class>
        </datatype>
        <dataFormat>NEWICK</dataFormat>
      </type>
      <format>
        <code proglang="perl">(defined $value) ? " $value" : ""</code>
        <code proglang="python">("" , " " + str(value))[value is not None]</code>
      </format>
      <argpos>2</argpos>
      <interface type="form" generated="true">
        <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <xhtml:legend>Second treeset</xhtml:legend>
          <xhtml:textarea name="input_2" cols="60" rows="7"> </xhtml:textarea>
        </xhtml:fieldset>
      </interface>
      <interface generated="true" type="job_input">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK " data-parametername="input_2">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Tree)
			</xhtml:i>
          <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117728296784::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
          <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117728296784::comment" class="commentText" style="display:none" mode="ajaxTarget">type any kind of text</xhtml:div>
        </div>
      </interface>
    </parameter>
    <parameter isstdout="1">
      <name>output</name>
      <prompt lang="en">Output treeset</prompt>
      <type>
        <datatype>
          <class>Tree</class>
        </datatype>
        <dataFormat>NEWICK</dataFormat>
      </type>
      <argpos>1</argpos>
      <filenames>
        <code proglang="perl">"nw_cat.out"</code>
        <code proglang="python">"nw_cat.out"</code>
      </filenames>
      <interface generated="true" type="job_output">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Tree" data-datatype-superclass="" data-biotype="" data-card="" data-formats="NEWICK " data-parametername="output">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Tree)
			</xhtml:i>
        </div>
      </interface>
    </parameter>
    <parameter isout="true">
      <name>stderr</name>
      <prompt>Standard error</prompt>
      <type>
        <datatype>
          <class>Report</class>
        </datatype>
      </type>
      <interface type="job_output" generated="true">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="stderr">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
        </div>
      </interface>
      <filenames>
        <code proglang="perl">"nw_cat.err"</code>
        <code proglang="python">"nw_cat.err"</code>
      </filenames>
    </parameter>
  </parameters>
</program>

