            </message>
            <code proglang="python">value &gt;= 0.</code>
          </ctrl>
          <ctrl>
            <message>
              <text lang="en">Value less than or equal to 100. is required</text>
            </message>
            <code proglang="python">value &lt;= 100.</code>
          </ctrl>
          <argpos>4</argpos>
