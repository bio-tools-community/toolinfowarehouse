              <class>Float</class>
            </datatype>
          </type>
          <vdef>
            <value>238.71</value>
          </vdef>
          <format>
            <code proglang="python">("", " -mean=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <ctrl>
            <message>
              <text lang="en">Value greater than or equal to 1. is required</text>
            </message>
            <code proglang="python">value &gt;= 1.</code>
          </ctrl>
          <ctrl>
            <message>
              <text lang="en">Value less than or equal to 10000. is required</text>
            </message>
            <code proglang="python">value &lt;= 10000.</code>
