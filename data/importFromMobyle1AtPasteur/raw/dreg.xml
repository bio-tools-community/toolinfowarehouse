<program>
  <head>
    <name>dreg</name>
    <package>
      <name>EMBOSS</name>
      <version>6.3.1</version>
      <doc>
        <title>EMBOSS</title>
        <description>
          <text lang="en">European Molecular Biology Open Software Suite</text>
        </description>
        <authors>Rice,P. Longden,I. and Bleasby, A.</authors>
        <reference>EMBOSS: The European Molecular Biology Open Software Suite (2000)  Rice,P. Longden,I. and Bleasby, A. Trends in Genetics 16, (6) pp276--277</reference>
        <sourcelink>http://emboss.sourceforge.net/download</sourcelink>
        <homepagelink>http://emboss.sourceforge.net</homepagelink>
      </doc>
    </package>
    <doc>
      <title>dreg</title>
      <description>
        <text lang="en">Regular expression search of nucleotide sequence(s)</text>
      </description>
      <doclink>http://bioweb2.pasteur.fr/docs/EMBOSS/dreg.html</doclink>
      <doclink>http://emboss.sourceforge.net/docs/themes</doclink>
    </doc>
    <command>dreg</command>
    <category>sequence:nucleic:motifs</category>
    <interface type="form" generated="true">
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Input section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <fieldset class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="1,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_sequence">
            <legend>sequence option</legend>
            <xhtml:textarea name="e_sequence" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
          <fieldset class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Pattern" data-datatype-superclass="AbstractText" data-biotype="DNA " data-card="" data-formats="" data-parametername="e_pattern">
            <legend>Regular expression pattern</legend>
            <xhtml:textarea name="e_pattern" cols="60" rows="7"> </xhtml:textarea>
          </fieldset>
        </xhtml:div>
      </fieldset>
      <fieldset xmlns="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <legend xmlns:xhtml="http://www.w3.org/1999/xhtml">Output section</legend>
        <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="dreg.report" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile">Name of the report file<xhtml:input name="e_outfile" type="text" value="dreg.report"/>
          </label>
          <label class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="SEQTABLE" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_rformat_outfile">Choose the report output format<xhtml:select name="e_rformat_outfile">
              <xhtml:option value="DASGFF">Dasgff</xhtml:option>
              <xhtml:option value="DBMOTIF">Dbmotif</xhtml:option>
              <xhtml:option value="DIFFSEQ">Diffseq</xhtml:option>
              <xhtml:option value="EMBL">Embl</xhtml:option>
              <xhtml:option value="EXCEL">Excel</xhtml:option>
              <xhtml:option value="FEATTABLE">Feattable</xhtml:option>
              <xhtml:option value="GENBANK">Genbank</xhtml:option>
              <xhtml:option value="GFF">Gff</xhtml:option>
              <xhtml:option value="LISTFILE">Listfile</xhtml:option>
              <xhtml:option value="MOTIF">Motif</xhtml:option>
              <xhtml:option value="NAMETABLE">Nametable</xhtml:option>
              <xhtml:option value="CODATA">Codata</xhtml:option>
              <xhtml:option value="REGIONS">Regions</xhtml:option>
              <xhtml:option value="SEQTABLE" selected="selected">Seqtable</xhtml:option>
              <xhtml:option value="SIMPLE">Simple</xhtml:option>
              <xhtml:option value="SRS">Srs</xhtml:option>
              <xhtml:option value="SWISS">Swiss</xhtml:option>
              <xhtml:option value="TABLE">Table</xhtml:option>
              <xhtml:option value="TAGSEQ">Tagseq</xhtml:option>
            </xhtml:select>
          </label>
        </xhtml:div>
      </fieldset>
    </interface>
    <interface type="job_input" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_input">
        <xhtml:legend>Input section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_sequence" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="1,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF ">
            <xhtml:i>
				(DNA Sequence)
			</xhtml:i>
          </div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_pattern" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Pattern" data-datatype-superclass="AbstractText" data-biotype="DNA " data-card="" data-formats="">
            <xhtml:i>
				(DNA Pattern)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="dreg.report" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_rformat_outfile" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="SEQTABLE" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="auto" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats=""/>
    </interface>
    <interface type="job_output" generated="true">
      <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml" class="minimizable" data-paragraphname="e_output">
        <xhtml:legend>Output section</xhtml:legend>
        <xhtml:div>
          <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="e_outfile_out" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Text" data-datatype-superclass="" data-biotype="" data-card="" data-formats="&#10;               ">
            <xhtml:i>
				(Text)
			</xhtml:i>
          </div>
        </xhtml:div>
      </xhtml:fieldset>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stdout" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
      <div xmlns="http://www.w3.org/1999/xhtml" data-parametername="stderr" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">
        <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
      </div>
    </interface>
  </head>
  <parameters>
    <paragraph>
      <name>e_input</name>
      <prompt lang="en">Input section</prompt>
      <parameters>
        <parameter issimple="1" ismandatory="1">
          <name>e_sequence</name>
          <prompt lang="en">sequence option</prompt>
          <type>
            <biotype>DNA</biotype>
            <datatype>
              <class>Sequence</class>
            </datatype>
            <dataFormat>EMBL</dataFormat>
            <dataFormat>FASTA</dataFormat>
            <dataFormat>GCG</dataFormat>
            <dataFormat>GENBANK</dataFormat>
            <dataFormat>NBRF</dataFormat>
            <dataFormat>CODATA</dataFormat>
            <dataFormat>RAW</dataFormat>
            <dataFormat>SWISSPROT</dataFormat>
            <dataFormat>GFF</dataFormat>
            <card>1,n</card>
          </type>
          <format>
            <code proglang="python">("", " -sequence=" + str(value))[value is not None]</code>
          </format>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>sequence option</xhtml:legend>
              <xhtml:textarea name="e_sequence" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="DNA " data-card="1,n" data-formats="EMBL FASTA GCG GENBANK NBRF CODATA RAW SWISSPROT GFF " data-parametername="e_sequence">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(DNA Sequence)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
        <parameter issimple="1" ismandatory="1">
          <name>e_pattern</name>
          <prompt lang="en">Regular expression pattern</prompt>
          <type>
            <biotype>DNA</biotype>
            <datatype>
              <class>Pattern</class>
              <superclass>AbstractText</superclass>
            </datatype>
          </type>
          <format>
            <code proglang="python">("", " -pattern=@" + str(value))[value is not None]</code>
          </format>
          <argpos>2</argpos>
          <interface type="form" generated="true">
            <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
              <xhtml:legend>Regular expression pattern</xhtml:legend>
              <xhtml:textarea name="e_pattern" cols="60" rows="7"> </xhtml:textarea>
            </xhtml:fieldset>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Pattern" data-datatype-superclass="AbstractText" data-biotype="DNA " data-card="" data-formats="" data-parametername="e_pattern">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(DNA Pattern)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <paragraph>
      <name>e_output</name>
      <prompt lang="en">Output section</prompt>
      <parameters>
        <parameter>
          <name>e_outfile</name>
          <prompt lang="en">Name of the report file</prompt>
          <type>
            <datatype>
              <class>Filename</class>
            </datatype>
          </type>
          <vdef>
            <value>dreg.report</value>
          </vdef>
          <format>
            <code proglang="python">("" , " -outfile=" + str(value))[value is not None]</code>
          </format>
          <argpos>3</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Name of the report file<xhtml:input xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_outfile" type="text" value="dreg.report"/>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="dreg.report" data-datatype="Filename" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_outfile"/>
          </interface>
        </parameter>
        <parameter>
          <name>e_rformat_outfile</name>
          <prompt lang="en">Choose the report output format</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <vdef>
            <value>SEQTABLE</value>
          </vdef>
          <vlist>
            <velem>
              <value>DASGFF</value>
              <label>Dasgff</label>
            </velem>
            <velem>
              <value>DBMOTIF</value>
              <label>Dbmotif</label>
            </velem>
            <velem>
              <value>DIFFSEQ</value>
              <label>Diffseq</label>
            </velem>
            <velem>
              <value>EMBL</value>
              <label>Embl</label>
            </velem>
            <velem>
              <value>EXCEL</value>
              <label>Excel</label>
            </velem>
            <velem>
              <value>FEATTABLE</value>
              <label>Feattable</label>
            </velem>
            <velem>
              <value>GENBANK</value>
              <label>Genbank</label>
            </velem>
            <velem>
              <value>GFF</value>
              <label>Gff</label>
            </velem>
            <velem>
              <value>LISTFILE</value>
              <label>Listfile</label>
            </velem>
            <velem>
              <value>MOTIF</value>
              <label>Motif</label>
            </velem>
            <velem>
              <value>NAMETABLE</value>
              <label>Nametable</label>
            </velem>
            <velem>
              <value>CODATA</value>
              <label>Codata</label>
            </velem>
            <velem>
              <value>REGIONS</value>
              <label>Regions</label>
            </velem>
            <velem>
              <value>SEQTABLE</value>
              <label>Seqtable</label>
            </velem>
            <velem>
              <value>SIMPLE</value>
              <label>Simple</label>
            </velem>
            <velem>
              <value>SRS</value>
              <label>Srs</label>
            </velem>
            <velem>
              <value>SWISS</value>
              <label>Swiss</label>
            </velem>
            <velem>
              <value>TABLE</value>
              <label>Table</label>
            </velem>
            <velem>
              <value>TAGSEQ</value>
              <label>Tagseq</label>
            </velem>
          </vlist>
          <format>
            <code proglang="python">("", " -rformat=" + str(value))[value is not None and value!=vdef]</code>
          </format>
          <argpos>4</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Choose the report output format<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="e_rformat_outfile">
                <xhtml:option value="DASGFF">Dasgff</xhtml:option>
                <xhtml:option value="DBMOTIF">Dbmotif</xhtml:option>
                <xhtml:option value="DIFFSEQ">Diffseq</xhtml:option>
                <xhtml:option value="EMBL">Embl</xhtml:option>
                <xhtml:option value="EXCEL">Excel</xhtml:option>
                <xhtml:option value="FEATTABLE">Feattable</xhtml:option>
                <xhtml:option value="GENBANK">Genbank</xhtml:option>
                <xhtml:option value="GFF">Gff</xhtml:option>
                <xhtml:option value="LISTFILE">Listfile</xhtml:option>
                <xhtml:option value="MOTIF">Motif</xhtml:option>
                <xhtml:option value="NAMETABLE">Nametable</xhtml:option>
                <xhtml:option value="CODATA">Codata</xhtml:option>
                <xhtml:option value="REGIONS">Regions</xhtml:option>
                <xhtml:option value="SEQTABLE" selected="selected">Seqtable</xhtml:option>
                <xhtml:option value="SIMPLE">Simple</xhtml:option>
                <xhtml:option value="SRS">Srs</xhtml:option>
                <xhtml:option value="SWISS">Swiss</xhtml:option>
                <xhtml:option value="TABLE">Table</xhtml:option>
                <xhtml:option value="TAGSEQ">Tagseq</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="SEQTABLE" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="e_rformat_outfile"/>
          </interface>
        </parameter>
        <parameter isout="1">
          <name>e_outfile_out</name>
          <prompt lang="en">outfile_out option</prompt>
          <type>
            <datatype>
              <class>Text</class>
            </datatype>
            <dataFormat>
              <ref param="e_rformat_outfile">
              </ref>
            </dataFormat>
          </type>
          <precond>
            <code proglang="python">e_rformat_outfile in ['DASGFF', 'DBMOTIF', 'DIFFSEQ', 'EMBL', 'EXCEL', 'FEATTABLE', 'GENBANK', 'GFF', 'LISTFILE', 'MOTIF', 'NAMETABLE', 'CODATA', 'REGIONS', 'SEQTABLE', 'SIMPLE', 'SRS', 'SWISS', 'TABLE', 'TAGSEQ']</code>
          </precond>
          <filenames>
            <code proglang="python">e_outfile</code>
          </filenames>
          <interface generated="true" type="job_output">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Text" data-datatype-superclass="" data-biotype="" data-card="" data-formats="&#10;               " data-parametername="e_outfile_out">
              <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Text)
			</xhtml:i>
            </div>
          </interface>
        </parameter>
      </parameters>
    </paragraph>
    <parameter ishidden="1">
      <name>auto</name>
      <prompt lang="en">Turn off any prompting</prompt>
      <type>
        <datatype>
          <class>String</class>
        </datatype>
      </type>
      <format>
        <code proglang="python">" -auto -stdout"</code>
      </format>
      <argpos>5</argpos>
      <interface generated="true" type="job_input">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="String" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="auto"/>
      </interface>
    </parameter>
    <parameter isstdout="true">
      <name>stdout</name>
      <prompt>Standard output</prompt>
      <type>
        <datatype>
          <class>Report</class>
        </datatype>
      </type>
      <interface type="job_output" generated="true">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="stdout">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
        </div>
      </interface>
      <filenames>
        <code proglang="perl">"dreg.out"</code>
        <code proglang="python">"dreg.out"</code>
      </filenames>
    </parameter>
    <parameter isout="true">
      <name>stderr</name>
      <prompt>Standard error</prompt>
      <type>
        <datatype>
          <class>Report</class>
        </datatype>
      </type>
      <interface type="job_output" generated="true">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="Report" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="stderr">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Report)
			</xhtml:i>
        </div>
      </interface>
      <filenames>
        <code proglang="perl">"dreg.err"</code>
        <code proglang="python">"dreg.err"</code>
      </filenames>
    </parameter>
  </parameters>
</program>

