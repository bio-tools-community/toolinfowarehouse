  <parameters>
    <parameter ismandatory="1" issimple="1">
      <name>seq</name>
      <prompt lang="en">Sequences File</prompt>
      <type>
        <datatype>
          <class>Sequence</class>
        </datatype>
        <dataFormat>FASTA</dataFormat>
      </type>
      <format>
        <code proglang="perl">" $value"</code>
        <code proglang="python">" "+str(value)</code>
      </format>
      <argpos>100</argpos>
      <interface type="form" generated="true">
        <xhtml:fieldset xmlns:xhtml="http://www.w3.org/1999/xhtml">
          <xhtml:legend>Sequences File</xhtml:legend>
          <xhtml:textarea name="seq" cols="60" rows="7"> </xhtml:textarea>
        </xhtml:fieldset>
      </interface>
      <interface generated="true" type="job_input">
        <div xmlns="http://www.w3.org/1999/xhtml" class="parameter &#10;&#9;&#9;&#9;&#9;mandatory&#10;&#9;&#9;&#9;&#9;" title="This parameter is mandatory" data-issimple="true" data-ismultiple="false" data-default-value="" data-datatype="Sequence" data-datatype-superclass="" data-biotype="" data-card="" data-formats="FASTA " data-parametername="seq">
          <xhtml:i xmlns:xhtml="http://www.w3.org/1999/xhtml">
				(Sequence)
			</xhtml:i>
        </div>
      </interface>
    </parameter>
    <paragraph>
      <name>control</name>
      <prompt lang="en">Control parameters</prompt>
      <parameters>
        <parameter>
          <name>cost</name>
          <prompt lang="en">Cost matrix (-c)</prompt>
          <type>
            <datatype>
              <class>Choice</class>
            </datatype>
          </type>
          <vdef>
            <value>null</value>
          </vdef>
          <vlist>
            <velem undef="1">
              <value>null</value>
              <label>Default: depends on sequences</label>
            </velem>
            <velem>
              <value>blosum30</value>
              <label>BLOSUM30</label>
            </velem>
            <velem>
              <value>blosum45</value>
              <label>BLOSUM45</label>
            </velem>
            <velem>
              <value>blosum62</value>
              <label>BLOSUM62</label>
            </velem>
            <velem>
              <value>pam160</value>
              <label>PAM160</label>
            </velem>
            <velem>
              <value>pam250</value>
              <label>PAM250</label>
            </velem>
            <velem>
              <value>unitcost</value>
              <label>Unitcost</label>
            </velem>
            <velem>
              <value>dna</value>
              <label>DNA</label>
            </velem>
            <velem>
              <value>rna</value>
              <label>RNA</label>
            </velem>
            <velem>
              <value>dnarna</value>
              <label>DNA/RNA</label>
            </velem>
          </vlist>
          <format>
            <code proglang="perl">(defined $value and $value ne $vdef)? " -c $value" : "" </code>
            <code proglang="python">( ""  , " -c " + str(value) )[ value is not None and value != vdef]</code>
          </format>
          <argpos>1</argpos>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Cost matrix (-c)<xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="cost">
                <xhtml:option value="null" selected="selected">Default: depends on sequences</xhtml:option>
                <xhtml:option value="blosum30">BLOSUM30</xhtml:option>
                <xhtml:option value="blosum45">BLOSUM45</xhtml:option>
                <xhtml:option value="blosum62">BLOSUM62</xhtml:option>
                <xhtml:option value="pam160">PAM160</xhtml:option>
                <xhtml:option value="pam250">PAM250</xhtml:option>
                <xhtml:option value="unitcost">Unitcost</xhtml:option>
                <xhtml:option value="dna">DNA</xhtml:option>
                <xhtml:option value="rna">RNA</xhtml:option>
                <xhtml:option value="dnarna">DNA/RNA</xhtml:option>
              </xhtml:select>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="null" data-datatype="Choice" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="cost"/>
          </interface>
        </parameter>
        <parameter>
          <name>gaps</name>
          <prompt lang="en">Penalize end gaps as internal gaps (-g)</prompt>
          <type>
            <datatype>
              <class>Boolean</class>
            </datatype>
          </type>
          <vdef>
            <value>0</value>
          </vdef>
          <format>
            <code proglang="perl"> ($value)? " -g ":""</code>
            <code proglang="python">("" , " -g ")[ value ]</code>
          </format>
          <argpos>1</argpos>
          <comment>
            <text lang="en">Default: free shift</text>
          </comment>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Penalize end gaps as internal gaps (-g)<xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117698437264::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
              <xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="gaps">
                <xhtml:option value="1">Yes</xhtml:option>
                <xhtml:option value="0" selected="selected">No</xhtml:option>
              </xhtml:select>
              <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117698437264::comment" class="commentText" style="display:none" mode="ajaxTarget">
                <xhtml:div>Default: free shift</xhtml:div>
              </xhtml:div>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="gaps">
              <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117698437264::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
              <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117698437264::comment" class="commentText" style="display:none" mode="ajaxTarget">
                <xhtml:div>Default: free shift</xhtml:div>
              </xhtml:div>
            </div>
          </interface>
        </parameter>
        <parameter>
          <name>approximate</name>
          <prompt lang="en">Use approximate cut positions (-a)</prompt>
          <type>
            <datatype>
              <class>Boolean</class>
            </datatype>
          </type>
          <vdef>
            <value>0</value>
          </vdef>
          <format>
            <code proglang="perl"> ($value)? " -a":""</code>
