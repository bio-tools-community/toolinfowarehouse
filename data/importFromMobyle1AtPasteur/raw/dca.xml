          <argpos>1</argpos>
          <comment>
            <text lang="en">On: FastDCA (use approximate cut positions); Off: slower, more accurate algorithm (search for exact cut positions)</text>
          </comment>
          <interface type="form" generated="true">
            <label xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="" data-datatype="" data-datatype-superclass="" data-biotype="" data-card="" data-formats="">Use approximate cut positions (-a)<xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117700377824::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
              <xhtml:select xmlns:xhtml="http://www.w3.org/1999/xhtml" name="approximate">
                <xhtml:option value="1">Yes</xhtml:option>
                <xhtml:option value="0" selected="selected">No</xhtml:option>
              </xhtml:select>
              <xhtml:div xmlns:xhtml="http://www.w3.org/1999/xhtml" id="idm140117700377824::comment" class="commentText" style="display:none" mode="ajaxTarget">
                <xhtml:div>On: FastDCA (use approximate cut positions); Off: slower, more accurate algorithm (search for exact cut positions)</xhtml:div>
              </xhtml:div>
            </label>
          </interface>
          <interface generated="true" type="job_input">
            <div xmlns="http://www.w3.org/1999/xhtml" class="parameter " title="" data-issimple="false" data-ismultiple="false" data-default-value="0" data-datatype="Boolean" data-datatype-superclass="" data-biotype="" data-card="" data-formats="" data-parametername="approximate">
              <xhtml:a xmlns:xhtml="http://www.w3.org/1999/xhtml" href="#idm140117700377824::comment" class="blindLink commentToggle" onclick="if (typeof portal=='undefined'){var target=document.getElementById(this.getAttribute('href').substr(1)); target.style.display=(target.style.display=='none') ? '':'none';}" title="click to expand/collapse contextual help">?</xhtml:a>
